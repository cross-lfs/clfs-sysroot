+
+void
+runtime64 (void)
+{
+  volatile double v1 = 28., v2 = 3., v3 = 9., v4 = 31., v5 = 3., v6 = 10.;
+  double     b64 = (double)((v1/v2-v3) - (v4/v5-v6));
+  _Decimal64 d64 = (double)((v1/v2-v3) - (v4/v5-v6));
